-- ::= import "lib/core.ic"
io ::= import "lib/io.ic"

PartOne ::= () -> int64 {
  test := [1721, 979, 366, 299, 675, 1456]
  n := 6 //test.length

  i := 0
  while (i < n) do {
    target := 2020 - test[i]
    j := i + 1
    while (j < n) do {
      if (target == test[j]) then {
        return test[i] * test[j]
      }
      j += 1
    }
    i += 1
  }
}


io.Print("Part 1: ")
io.Print(PartOne())
io.Print("\n")
