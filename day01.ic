-- ::= import "lib/core.ic"
io ::= import "lib/io.ic"

PartOne ::= (nums: *[6; int64]) -> int64 {
  n := (@nums):?.length as int64

  i := 0
  while (i < n) do {
    target := 2020 - (@nums)[i]
    j := i + 1
    while (j < n) do {
      if (target == (@nums)[j]) then {
        return (@nums)[i] * (@nums)[j]
      }
      j += 1
    }
    i += 1
  }
}


test := [1721, 979, 366, 299, 675, 1456]
io.Print("Part 1: ")
io.Print(PartOne(&test))
io.Print("\n")
