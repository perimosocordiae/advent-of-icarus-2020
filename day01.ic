-- ::= import "core.ic"
cstdio ::= import "cstdio.ic"
io ::= import "io.ic"

PartOne ::= (nums: $nums) -> i64 {
  n := (@nums):?.length as i64

  for (0, n) do [i: i64] {
    target := 2020 - (@nums)[i]
    for (i + 1, n) do [j: i64] {
      if (target == (@nums)[j]) then {
        return (@nums)[i] * (@nums)[j]
      }
    }
  }
}


test := [1721, 979, 366, 299, 675, 1456]
io.Print("Part 1 (test): ")
io.Print(PartOne(&test))
io.Print("\n")

